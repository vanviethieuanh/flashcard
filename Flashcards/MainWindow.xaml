<Window x:Class="Flashcards.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flashcards"
        xmlns:uc="clr-namespace:Flashcards.UC"
        mc:Ignorable="d" WindowStyle="None" SizeChanged="Window_SizeChanged"
        ResizeMode="CanResizeWithGrip" WindowStartupLocation="CenterScreen" Closing="Window_Closing"
        Title="Flashcards" Height="506" Width="1034" MinHeight="550" MinWidth="685" BorderThickness="0,0,0,0" 
        AllowsTransparency="True" Icon="Icon\icon2.ico" IsEnabledChanged="Window_IsEnabledChanged"
        BorderBrush="#FF007ACC" Background="Transparent">
    <Window.Resources>
        <Style TargetType="Ellipse">
            <Setter Property="Stroke" Value="{DynamicResource Main_color.dark}"/>
            <Setter Property="StrokeThickness" Value="3"/>
            <Setter Property="Width" Value="15"/>
            <Setter Property="Height" Value="15"/>
            <Setter Property="Fill" Value="White"/>
        </Style>

        <Style TargetType="Line">
            <Setter Property="Stroke" Value="{DynamicResource Main_color.light}"/>
            <Setter Property="StrokeThickness" Value="5"/>
            <Setter Property="StrokeStartLineCap" Value="Round"/>
            <Setter Property="StrokeEndLineCap" Value="Round"/>
        </Style>
        <Style TargetType="Label" x:Key="Lableh2">
            <Setter Property="Padding" Value="0"/>
            <Setter Property="FontSize" Value="25"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
        </Style>
        <Style TargetType="Label">
            <Setter Property="FontSize" Value="17"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="Padding" Value="0"/>
        </Style>
        <Style TargetType="TabControl" x:Key="Large_tabcontrol">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="TabControl">
                        <Grid>
                            <Border SnapsToDevicePixels="True" Margin="200,0,0,0" Background="{TemplateBinding Background}"/>
                            <StackPanel HorizontalAlignment="Left" IsItemsHost="True" Width="200" Margin="0,0,0,0"/>
                            <ContentPresenter ContentSource="SelectedContent" Margin="200,0,0,0"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="TabControl" x:Key="Small_tabcontrol">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="TabControl">
                        <Grid>
                            <Border SnapsToDevicePixels="True" Margin="65,0,0,0" Background="{TemplateBinding Background}"/>
                            <StackPanel HorizontalAlignment="Left" IsItemsHost="True" Width="65"/>
                            <ContentPresenter ContentSource="SelectedContent" Margin="65,0,0,0"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="TabItem">
            <Setter Property="SnapsToDevicePixels" Value="True"/>
            <Setter Property="OpacityMask" Value="{DynamicResource Main_color.light}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="TabItem">
                        <Grid Background="Transparent" Name="grid_colortabitem">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="60"/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid Name="highlight" Margin="0,5"></Grid>
                            <Grid Grid.Column="1">
                                <Path SnapsToDevicePixels="True" Name="path" Data="{DynamicResource ResourceKey=DataPath}"
                                      Fill="White" HorizontalAlignment="Center" VerticalAlignment="Center" 
                                      LayoutTransform="{DynamicResource ResourceKey=scale}" 
                                      OpacityMask="{TemplateBinding OpacityMask}" Margin="{TemplateBinding Padding}">
                                </Path>
                            </Grid>
                            <Grid Grid.Column="2">
                                <Label Name="content" Style="{DynamicResource label_tabcontrol_item}" 
                                       Content="{TemplateBinding Header}" Opacity="0.7" Margin="0"/>
                            </Grid>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter TargetName="grid_colortabitem" Property="Background" Value="#FF1E1E1E"/>
                                <Setter TargetName="path" Property="Fill" Value="{Binding ElementName=path,Path=OpacityMask}"/>
                                <Setter TargetName="content" Property="Opacity" Value="1"/>
                                <Setter TargetName="highlight" Property="Background" Value="{Binding ElementName=path,Path=OpacityMask}"/>
                            </Trigger>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsSelected" Value="False"/>
                                    <Condition Property="IsMouseOver" Value="True"/>
                                </MultiTrigger.Conditions>
                                <MultiTrigger.Setters>
                                    <Setter TargetName="grid_colortabitem" Property="Background" Value="#FF2D2D2D"/>
                                </MultiTrigger.Setters>
                            </MultiTrigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style TargetType="ListViewItem" x:Key="SingleSelection">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ListViewItem}">
                        <Grid Background="White" Height="30" Name="item">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30*"/>
                                <ColumnDefinition Width="12*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="Delete" Name="it_delete" Click="mit_dictionary_delWord_Click"/>
                                    <MenuItem Header="Move" Name="it_move" Click="mit_dictionary_moveWord_Click"/>
                                    <Separator Margin="0,2,3,2"/>
                                    <MenuItem Header="Select" Name="it_select" Click="it_select_Click"/>
                                </ContextMenu>
                            </Grid.ContextMenu>
                            <TextBlock Name="day" Grid.Column="1" VerticalAlignment="Center" FontSize="13" Foreground="Gray" Text="{Binding PracticeDay}" Padding="10,0,0,0"/>

                            <Grid HorizontalAlignment="Left">
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Left" Padding="10,0,0,0" FontSize="15" Text="{Binding WordDic}" Name="title"></TextBlock>
                            </Grid>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="true">
                                <Setter Property="Background" Value="#FFB9B9B9" TargetName="item"/>
                                <Setter Property="Foreground" Value="#fff" TargetName="title"/>
                                <Setter Property="Foreground" Value="White" TargetName="day"/>
                            </Trigger>
                            <Trigger Property="IsSelected" Value="true">
                                <Setter Property="Background" Value="Gray" TargetName="item"/>
                                <Setter Property="Foreground" Value="#fff" TargetName="title"/>
                                <Setter Property="Foreground" Value="White" TargetName="day"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <EventSetter Event="MouseDoubleClick" Handler="HandleDoubleClick"/>
        </Style>
        <Style TargetType="ListViewItem" x:Key="MultipleSelection">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ListViewItem}">
                        <Grid Background="White" Height="30" Name="item">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="30*"/>
                                <ColumnDefinition Width="12*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="Delete" Name="it_delete" Click="mit_dictionary_delWord_Click"/>
                                    <MenuItem Header="Move" Name="it_move" Click="mit_dictionary_moveWord_Click"/>
                                </ContextMenu>
                            </Grid.ContextMenu>
                            <Grid Name="tick" Visibility="Hidden">
                                <Path Data="F1 M 34.848,10.9387L 19.32,29.3333L 10.6667,20.136" Stroke="White" StrokeThickness="3">
                                    <Path.LayoutTransform>
                                        <ScaleTransform ScaleX="0.7" ScaleY="0.7"/>
                                    </Path.LayoutTransform>
                                </Path>
                            </Grid>
                            <Grid HorizontalAlignment="Left" Grid.Column="1">
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Left" Padding="10,0,0,0" FontSize="15" Text="{Binding WordDic}" Name="title"></TextBlock>
                            </Grid>
                            <TextBlock Grid.Column="2" Name="day" VerticalAlignment="Center" FontSize="13" Foreground="Gray" Text="{Binding PracticeDay}" Margin="0,6" Padding="10,0,0,0"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="true">
                                <Setter Property="Background" Value="#f1f1f1" TargetName="item"/>
                            </Trigger>
                            <Trigger Property="IsSelected" Value="true">
                                <Setter Property="Visibility" Value="Visible" TargetName="tick"/>
                                <Setter Property="Background" Value="Gray" TargetName="item"/>
                                <Setter Property="Foreground" Value="#fff" TargetName="title"/>
                                <Setter Property="Foreground" Value="White" TargetName="day"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="ListViewItem" x:Key="ListView_Dictionary">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ListViewItem}">
                        <Grid Background="White" Height="30" Name="item">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30*"/>
                                <ColumnDefinition Width="12*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="Delete" Name="mit_delete" Click="mit_dictionary_deleteDic_Click"/>
                                    <MenuItem Header="Rename" Name="mit_rename" Click="nit_dictionary_rename_Click"/>
                                </ContextMenu>
                            </Grid.ContextMenu>
                            <TextBlock Name="day" Grid.Column="1" VerticalAlignment="Center" FontSize="13" Foreground="Gray" Text="{Binding NumberOfWord}" Padding="10,0,0,0"/>

                            <Grid HorizontalAlignment="Left">
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Left" Padding="10,0,0,0" FontSize="15" Text="{Binding NameOfDictionary}" Name="title"></TextBlock>
                            </Grid>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="true">
                                <Setter Property="Background" Value="#f1f1f1" TargetName="item"/>
                            </Trigger>
                            <Trigger Property="IsSelected" Value="true">
                                <Setter Property="Background" Value="Gray" TargetName="item"/>
                                <Setter Property="Foreground" Value="#fff" TargetName="title"/>
                                <Setter Property="Foreground" Value="White" TargetName="day"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <EventSetter Event="MouseDoubleClick" Handler="ListDicitonary_MouseDoubleClick"/>
        </Style>
    </Window.Resources>
    <Grid Margin="7" Name="Boss">
        <Grid.Effect>
            <DropShadowEffect Opacity="0.3" Direction="270" ShadowDepth="3"></DropShadowEffect>
        </Grid.Effect>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <MediaElement LoadedBehavior="Manual" Name="Speaker" Width="0" Height="0" VerticalAlignment="Top" HorizontalAlignment="Left"></MediaElement>
        <Grid Grid.Row="0" Panel.ZIndex="1">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="200"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid Background="{DynamicResource Main_color.dark}">
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
