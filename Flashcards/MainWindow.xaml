                <Grid Background="{DynamicResource Main_color.light}" Grid.Column="1" MouseDown="Grid_MouseDown">
                    <Label Grid.ColumnSpan="2" Foreground="#232323" 
                           FontSize="25" Margin="0,-2,0,0" 
                           VerticalContentAlignment="Center"
                           HorizontalAlignment="Center">Flashcards</Label>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" FlowDirection="RightToLeft">
                        <Button Width="30" Name="btn_Close" Click="btn_Close_Click" SnapsToDevicePixels="True"
                                ToolTip="Close">
                            <Button.Resources>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="SnapsToDevicePixels" Value="True"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type Button}">
                                                <Grid Background="Transparent" Opacity="0.7" Name="back">
                                                    <Rectangle Width="2" Height="13" Fill="#232323">
                                                        <Rectangle.LayoutTransform>
                                                            <RotateTransform Angle="45"></RotateTransform>
                                                        </Rectangle.LayoutTransform>
                                                    </Rectangle>
                                                    <Rectangle Width="13" Height="2" Fill="#232323">
                                                        <Rectangle.LayoutTransform>
                                                            <RotateTransform Angle="45"></RotateTransform>
                                                        </Rectangle.LayoutTransform>
                                                    </Rectangle>
                                                </Grid>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="IsMouseOver" Value="True">
                                                        <Setter TargetName="back" Property="Opacity" Value="1"/>
                                                        <Setter TargetName="back" Property="Background" Value="#33FFFFFF"/>
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </Button.Resources>
                        </Button>
                        <Border Height="20" Width="1" BorderThickness="0" Background="#444444" Opacity="0.3"></Border>
                        <ToggleButton Width="30" SnapsToDevicePixels="True" Name="full" Click="full_Click" IsChecked="False">
                            <ToggleButton.Resources>
                                <Style TargetType="{x:Type ToggleButton}">
                                    <Setter Property="SnapsToDevicePixels" Value="True"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type ToggleButton}">
                                                <Grid Name="back" Opacity="0.7" OpacityMask="#232323">
                                                    <Grid Background="Transparent" Name="Restore_Down" Visibility="Visible">
                                                        <Border BorderThickness="1,2,1,1" BorderBrush="{Binding ElementName=back, Path=OpacityMask}" Width="7" Height="7" Margin="0,0,-3,-3"></Border>
                                                        <Border Margin="10,10,14,18" BorderThickness="0" Background="{Binding ElementName=back, Path=OpacityMask}" Width="6" Height="2"></Border>
                                                        <Border Margin="3,0,0,6" BorderThickness="0" Background="{Binding ElementName=back, Path=OpacityMask}" Width="1" Height="3"></Border>
                                                        <Border Margin="10,16,17,13" BorderThickness="0" Background="{Binding ElementName=back, Path=OpacityMask}" Width="3" Height="1"></Border>
                                                        <Border Margin="10,12,19,14" BorderThickness="0" Background="{Binding ElementName=back, Path=OpacityMask}" Width="1" Height="4"></Border>
                                                    </Grid>
                                                    <Grid Background="Transparent" Name="Maximize" Visibility="Hidden">
                                                        <Border BorderThickness="1,3,1,1" BorderBrush="{Binding ElementName=back, Path=OpacityMask}" Width="9" Height="9"></Border>
                                                    </Grid>
                                                </Grid>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="IsChecked" Value="True">
                                                        <!--Maximum-->
                                                        <Setter TargetName="Restore_Down" Property="Visibility" Value="Visible"/>
                                                        <Setter TargetName="Maximize" Property="Visibility" Value="Hidden"/>
                                                    </Trigger>
                                                    <Trigger Property="IsChecked" Value="False">
                                                        <!--Else-->
                                                        <Setter TargetName="Restore_Down" Property="Visibility" Value="Hidden"/>
                                                        <Setter TargetName="Maximize" Property="Visibility" Value="Visible"/>
                                                    </Trigger>
                                                    <Trigger Property="IsMouseOver" Value="true">
                                                        <Setter TargetName="back" Property="Opacity" Value="1"/>
                                                        <Setter TargetName="back" Property="Background" Value="#33FFFFFF"/>
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </ToggleButton.Resources>
                        </ToggleButton>
                        <Border Height="20" Width="1" BorderThickness="0" Background="#444444" Opacity="0.3"></Border>
                        <Button Width="30" SnapsToDevicePixels="True" Name="btn_Minimize" Click="btn_Minimize_Click"
                                ToolTip="Minimize">
                            <Button.Resources>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="SnapsToDevicePixels" Value="True"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
